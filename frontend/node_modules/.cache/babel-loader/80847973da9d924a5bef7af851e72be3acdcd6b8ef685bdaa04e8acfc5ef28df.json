un? Login</router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      username: '',\r\n      password: '',\r\n      confirmPassword: ''\r\n    };\r\n  },\r\n  methods: {\r\n    register() {\r\n      if (this.password === this.confirmPassword) {\r\n        const user = {\r\n          email: this.email,\r\n          username: this.username,\r\n          password: this.password\r\n        };\r\n\r\n        localStorage.setItem('user', JSON.stringify(user)); // Menyimpan data pengguna di localStorage\r\n        alert('Pendaftaran berhasil! Silakan login.');\r\n        this.$router.push('/login'); // Redirect ke halaman login setelah registrasi berhasil\r\n      } else {\r\n        alert('Password dan konfirmasi password tidak cocok.');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ninput {\r\n  margin: 10px 0;\r\n  padding: 8px;\r\n  width: 100%;\r\n}\r\n\r\nbutton {\r\n  background-color: #28a745;\r\n  color: white;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1C,MAAMG,IAAG,GAAI;UACXN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC;QAEDK,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACJ,IAAI,CAAC,CAAC,EAAE;QACpDK,KAAK,CAAC,sCAAsC,CAAC;QAC7C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;MAC/B,OAAO;QACLF,KAAK,CAAC,+CAA+C,CAAC;MACxD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}