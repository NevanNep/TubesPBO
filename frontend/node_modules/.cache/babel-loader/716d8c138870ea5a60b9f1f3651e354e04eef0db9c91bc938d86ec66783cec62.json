{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nexport default {\n  props: ['cartItems'],\n  data() {\n    return {\n      // Salin prop ke data lokal untuk menghindari mutasi langsung\n      localCartItems: [...this.cartItems]\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.localCartItems.reduce((sum, item) => sum + item.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    removeItem(item) {\n      const index = this.localCartItems.indexOf(item);\n      if (index > -1) {\n        this.localCartItems.splice(index, 1);\n      }\n      // Kirim perubahan kembali ke parent (induk)\n      this.$emit('update-cart', this.localCartItems);\n    }\n  }\n};","map":{"version":3,"names":["props","data","localCartItems","cartItems","computed","totalPrice","reduce","sum","item","price","quantity","methods","removeItem","index","indexOf","splice","$emit"],"sources":["C:\\Users\\USER\\scentify\\src\\components\\CartPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Keranjang Belanja</h2>\r\n    <ul>\r\n      <li v-for=\"item in localCartItems\" :key=\"item.id\">\r\n        {{ item.name }} - Rp {{ item.price }} x {{ item.quantity }} = Rp {{ item.price * item.quantity }}\r\n        <button @click=\"removeItem(item)\">Hapus</button>\r\n      </li>\r\n    </ul>\r\n    <p v-if=\"localCartItems.length === 0\">Keranjang Anda kosong.</p>\r\n    <p>Total: Rp {{ totalPrice }}</p>\r\n    <router-link to=\"/payment\">Lanjut ke Pembayaran</router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: ['cartItems'],\r\n  data() {\r\n    return {\r\n      // Salin prop ke data lokal untuk menghindari mutasi langsung\r\n      localCartItems: [...this.cartItems]\r\n    };\r\n  },\r\n  computed: {\r\n    totalPrice() {\r\n      return this.localCartItems.reduce((sum, item) => sum + item.price * item.quantity, 0);\r\n    }\r\n  },\r\n  methods: {\r\n    removeItem(item) {\r\n      const index = this.localCartItems.indexOf(item);\r\n      if (index > -1) {\r\n        this.localCartItems.splice(index, 1);\r\n      }\r\n      // Kirim perubahan kembali ke parent (induk)\r\n      this.$emit('update-cart', this.localCartItems);\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";;AAgBA,eAAe;EACbA,KAAK,EAAE,CAAC,WAAW,CAAC;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,cAAc,EAAE,CAAC,GAAG,IAAI,CAACC,SAAS;IACpC,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACH,cAAc,CAACI,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,KAAI,GAAID,IAAI,CAACE,QAAQ,EAAE,CAAC,CAAC;IACvF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAACJ,IAAI,EAAE;MACf,MAAMK,KAAI,GAAI,IAAI,CAACX,cAAc,CAACY,OAAO,CAACN,IAAI,CAAC;MAC/C,IAAIK,KAAI,GAAI,CAAC,CAAC,EAAE;QACd,IAAI,CAACX,cAAc,CAACa,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;MACtC;MACA;MACA,IAAI,CAACG,KAAK,CAAC,aAAa,EAAE,IAAI,CAACd,cAAc,CAAC;IAChD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}